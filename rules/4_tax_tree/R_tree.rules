rule create_tree_in_R :
    conda:
        "../../envs/Tree_making_R.yml"
    input:
        seqs_path = "{tool}/2_denoised/dna-sequences.fasta"
    output:
        tree_path = "{tool}/4_tree/tree.nwk"
    log:
        logging_folder + "{tool}/4_tree/tree.txt"
    threads:
        1
    script:
        "scripts/tree_in_r.R"
